.container{
    background-color: #B4B4B4;
    height: 0;
}
.display-column{
    align-items: center;
    display: flex;
    flex-direction: column;
    margin-top: 150rpx;
}
.title_big{
    font-size: 46rpx;
}
.title_marign{
    color: #B4B4B4;
    margin: 20rpx 0;
    font-size: 30rpx;

}
.input-box{
    margin-top: 20rpx;
    background-color: #fff;
    margin: 0 auto;
    padding: 20rpx 0;
    border-radius: 10rpx;
    width: 90%;
}
.input-box input{
    width: 100%;
    height: 100%;
    text-align: center;
    border:1px solid #f7d408;
    border-radius: 50rpx;
    height: 30px;
}
.endride {
    text-align: center;
    margin-top: 40rpx;
    position: relative;
    left: 30rpx;
    font-size: 40rpx;
    /*padding: rpx 0;*/
}
.btn-action{
    width: 90%;
    height: 36px;
    border-radius: 50rpx;
    border:1px solid lightgray;
    background-color: #B4B4B4;
    padding: 31rpx 0 0  0;
}